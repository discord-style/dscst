{
   "name": "dscst",
   "version": "1.0.0",
   "description": "asynchronous api wrapper for discord.style built with typescript",
   "main": "dist/index.js",
   "types": "dist/index.d.ts",
   "prepublish": "tsc",
   "scripts": {
      "test": "ts-node src/test.ts",
      "test:nodemon": "nodemon",
      "test:nodemon:run": "node -r ts-node/register ./src/test.ts",
      "build": "tsc"
   },
   "nodemonConfig": {
      "ignore": [
         "**/*.test.ts",
         "**/*.spec.ts",
         ".git",
         "node_modules"
      ],
      "watch": [
         "src"
      ],
      "exec": "npm run test:nodemon:run",
      "ext": "ts"
   },
   "keywords": [
      "api",
      "wrapper",
      "dscst",
      "discord.style",
      "discord",
      "style"
   ],
   "author": "tyler",
   "license": "MIT",
   "devDependencies": {
      "@types/axios": "^0.14.0",
      "@types/node": "^14.14.37",
      "dotenv": "^8.2.0",
      "nodemon": "^2.0.7",
      "ts-node": "^9.1.1",
      "typescript": "^4.2.4"
   },
   "dependencies": {
      "axios": "^0.21.1"
   }
}
